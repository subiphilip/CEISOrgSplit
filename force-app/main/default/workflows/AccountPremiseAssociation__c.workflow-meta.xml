    <rules>
        <fullName>AEP Meter Data Request</fullName>
        <actions>
            <name>Send_AEP_LOA</name>
            <type>Alert</type>
        </actions>
        <actions>
            <name>Get_City_State_Zip_onto_LOA</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>Get_ESID_onto_LOA</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>Get_Mailing_Address_onto_LOA</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>Get_Name_onto_LOA</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>Get_Phone_onto_LOA</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>Get_Service_Address_onto_LOA</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>Meter_Data_Status_In_Progress</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>LOA_submitted_for_customer</name>
            <type>Task</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>AccountPremiseAssociation__c.Meter_Data_Permission__c</field>
            <operation>equals</operation>
            <value>True</value>
        </criteriaItems>
        <criteriaItems>
            <field>AccountPremiseAssociation__c.TDSP__c</field>
            <operation>contains</operation>
            <value>AEP</value>
        </criteriaItems>
        <description>Customer Requests meter data, so we need to send an LOA</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>Daily Usage Completed Timestamp</fullName>
        <actions>
            <name>Set_Daily_request_completed_timestamp</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <booleanFilter>1 OR 2</booleanFilter>
        <criteriaItems>
            <field>AccountPremiseAssociation__c.Meter_Data_Daily_Status__c</field>
            <operation>equals</operation>
            <value>Complete</value>
        </criteriaItems>
        <criteriaItems>
            <field>AccountPremiseAssociation__c.Meter_Data_Daily_Status__c</field>
            <operation>contains</operation>
            <value>Error</value>
        </criteriaItems>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>Daily Usage Timestamp</fullName>
        <actions>
            <name>Set_daily_request_start_timestamp</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>AccountPremiseAssociation__c.Meter_Data_Daily_Status__c</field>
            <operation>equals</operation>
            <value>In Progress</value>
        </criteriaItems>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>Daily Usage%2FTOU Profile Completed First Time</fullName>
        <actions>
            <name>APA_ID_for_TOU_profile_to_informatica</name>
            <type>OutboundMessage</type>
        </actions>
        <active>true</active>
        <booleanFilter>1 and 2</booleanFilter>
        <criteriaItems>
            <field>AccountPremiseAssociation__c.Meter_Data_Daily_Status__c</field>
            <operation>equals</operation>
            <value>Complete</value>
        </criteriaItems>
        <criteriaItems>
            <field>AccountPremiseAssociation__c.TOU_Profile_Complete__c</field>
            <operation>equals</operation>
            <value>False</value>
        </criteriaItems>
        <description>sets off a flow to create a TOU profile</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
