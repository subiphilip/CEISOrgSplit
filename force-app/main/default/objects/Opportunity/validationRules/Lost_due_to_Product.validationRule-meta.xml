<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Lost_due_to_Product</fullName>
    <active>false</active>
    <errorConditionFormula>AND(TEXT(Lost_Reasoning__c) = &quot;Product&quot;, ISBLANK(Lost_Notes__c ))</errorConditionFormula>
