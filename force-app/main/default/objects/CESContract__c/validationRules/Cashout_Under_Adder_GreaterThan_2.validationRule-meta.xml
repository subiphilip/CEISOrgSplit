<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Cashout_Under_Adder_GreaterThan_2</fullName>
    <active>false</active>
    <errorConditionFormula>OR( 
Cashout_Under_Adder__c &lt; -1, 
Cashout_Under_Adder__c &gt; 2 
)</errorConditionFormula>
    <errorDisplayField>Cashout_Under_Adder__c</errorDisplayField>
    <errorMessage>CESC43: Cashout Under Adder must be between ($1.00) and $2.00 per MMBtu</errorMessage>
</ValidationRule>
