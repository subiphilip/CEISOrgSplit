<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Cashout_Over_Margin_GreaterThan_2</fullName>
    <active>false</active>
    <errorConditionFormula>OR( 
Cashout_Over_Margin__c &lt; -1, 
Cashout_Over_Margin__c &gt; 2 
)</errorConditionFormula>
    <errorDisplayField>Cashout_Over_Margin__c</errorDisplayField>
    <errorMessage>CESC37: Cashout Over Margin must be between ($1.00) and $2.00 per MMBtu</errorMessage>
</ValidationRule>
