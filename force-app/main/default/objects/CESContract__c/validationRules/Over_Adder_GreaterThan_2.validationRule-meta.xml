<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Over_Adder_GreaterThan_2</fullName>
    <active>false</active>
    <errorConditionFormula>OR( 
Over_Adder__c &lt; -1, 
Over_Adder__c &gt; 3
)</errorConditionFormula>
    <errorDisplayField>Over_Adder__c</errorDisplayField>
    <errorMessage>CESC11: Over Adder must be between ($1.00) and $2.00 per MMBtu</errorMessage>
</ValidationRule>
