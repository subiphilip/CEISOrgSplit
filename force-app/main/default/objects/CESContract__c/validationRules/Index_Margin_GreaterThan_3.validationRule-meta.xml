<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Index_Margin_GreaterThan_3</fullName>
    <active>false</active>
    <errorConditionFormula>OR( 
Margin_on_Index__c &lt; -1, 
Margin_on_Index__c &gt; 3 
)</errorConditionFormula>
    <errorDisplayField>Margin_on_Index__c</errorDisplayField>
    <errorMessage>CESC07: Margin on Index must be between or ($1.00) and $3.00 per MMBtu</errorMessage>
</ValidationRule>
