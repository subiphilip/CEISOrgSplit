<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Start_Date_first_of_the_month</fullName>
    <active>true</active>
    <errorConditionFormula>DAY(Start_Date__c) &gt; 1</errorConditionFormula>
