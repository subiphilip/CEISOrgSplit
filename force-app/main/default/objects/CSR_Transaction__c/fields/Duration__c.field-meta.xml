<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Duration__c</fullName>
    <description>How long did the TrueCost conversation last? (minutes)</description>
    <externalId>false</externalId>
    <formula>min((Stop__c - Start__c)*24*60, 10)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>How long did the TrueCost conversation last? (minutes)</inlineHelpText>
    <label>Duration</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
