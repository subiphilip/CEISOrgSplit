<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Euler_required_for_insurace</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
ISPICKVAL(Credit_Type__c, &quot;Insurance Coverage&quot;) ,
 ISBLANK( Euhler_RFC__c ) 
)</errorConditionFormula>
    <errorDisplayField>Euhler_RFC__c</errorDisplayField>
    <errorMessage>Credit05: Euhler RFC# is required for insurance coverage.</errorMessage>
</ValidationRule>
