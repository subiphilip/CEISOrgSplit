<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Passwords_must_match</fullName>
    <active>true</active>
    <errorConditionFormula>AND(NOT(ISBLANK(Password__c)), Confirm_Password__c != Password__c)</errorConditionFormula>
    <errorMessage>Enrollment05: Your password must match.</errorMessage>
</ValidationRule>
