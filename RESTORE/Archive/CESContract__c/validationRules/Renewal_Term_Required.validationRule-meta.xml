<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Renewal_Term_Required</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
 Auto_renew__c ,
 ISBLANK( Renewal_Term__c )
)</errorConditionFormula>
    <errorDisplayField>Renewal_Term__c</errorDisplayField>
    <errorMessage>CESC29: Renewal Term is required if Auto Renewal is selected</errorMessage>
</ValidationRule>
