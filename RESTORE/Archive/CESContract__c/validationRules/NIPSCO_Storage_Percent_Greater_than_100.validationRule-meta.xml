<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>NIPSCO_Storage_Percent_Greater_than_100</fullName>
    <active>false</active>
    <errorConditionFormula>AND(
NOT( 
ISBLANK( NIPSCO_Storage_Percent__c ) 
),OR(NIPSCO_Storage_Percent__c &gt;1, NIPSCO_Storage_Percent__c &lt; 0)
 )</errorConditionFormula>
    <errorDisplayField>NIPSCO_Storage_Percent__c</errorDisplayField>
    <errorMessage>CESC09: NIPSCO Storage Percent must be between 0 and 100</errorMessage>
</ValidationRule>
